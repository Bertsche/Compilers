{
	boolean b
	int x
	int y
	string r
	r = "setting boolean to false"
	y=0
	b = true
	while(b ==true)
	{
		print(y)
		y = 1 + y

		if(y == 9)
		{
			print(r)
			b = false
		}
	}

	print (y)
}
$



