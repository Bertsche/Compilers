{
	int x
	int y
	boolean b
	b = false
	x = 2
	y = 5
	if(b == true)
	{
		print(y)
		print(x)
	}
		print(x)
		print(y)

}
$


------------------------

{
	string a
	string b
	string e
	a="hello"
	b = "hello"
	e = "this should not print"
	if(a == a)
	{
		print(a)
	}
	if(a==b)
	{
		print(e)
	}
	if(a != b)
	{
		print(a)
		print("printing literal works")
	}

}
$


